<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#095E57</item>
        <item name="colorPrimaryDark">#09524C</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppNoActionBar" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimaryDark">#09524C</item>
    </style>

    <style name="NoTitleBarStyle" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimaryDark">#09524C</item>
        <item name="android:windowDisablePreview">true</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>
</resources>